@prefix : <#>.
@prefix acl: <http://www.w3.org/ns/auth/acl#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix target: <./>.

:Read
    a acl:Authorization;
    acl:accessTo target:;
    acl:default target:;
    acl:agent </profile/card#me>;
    acl:agent <https://test.solid.community/profile/card#me>;
    acl:mode acl:Read.

:ReadWrite
    a acl:Authorization;
    acl:accessTo target:;
    acl:default target:;
    acl:agentClass acl:AuthenticatedAgent;
    acl:mode acl:Read, acl:Write.

:ReadWriteControl
    a acl:Authorization;
    acl:accessTo target:;
    acl:default target:;
    acl:agentClass foaf:Agent;
    acl:mode acl:Read, acl:Write, acl:Control.
