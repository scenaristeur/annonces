log(images)\r\n      //  this.scan.images = images\r\n      console.log(this.scan)\r\n      images.forEach(async (item, i) => {\r\n        await this.recognize(item)\r\n        console.log(i)\r\n      });\r\n\r\n    },\r\n    recognize(url){\r\n\r\n      Tesseract.recognize(\r\n        //'https://tesseract.projectnaptha.com/img/eng_bw.png',\r\n        url,\r\n        'eng',\r\n        { logger: m => {\r\n          console.log(\"logger\",m)\r\n          this.logger = m\r\n        } }\r\n      ).then(({ data: { text } }) => {\r\n        console.log(\"text\", text);\r\n        this.text += text\r\n      })\r\n    },\r\n    resetText()\r\n    {\r\n      this.logger = {}\r\n      this.text = \"\"\r\n    }\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/scanner"}]}